<div class="app-main__inner">


<form action="/addUser" method="post">

    <label for="user"> Select User </label>

    <select name="user">
        <% for(var i=0; i < result.length; i++) { %>
            <option value=" <%= result[i].userId %> ">
                <%= result[i].userName %>
            </option>
            <% } %>
    </select>

    <input type="submit" value="Add" > </input>

</form>



</div>